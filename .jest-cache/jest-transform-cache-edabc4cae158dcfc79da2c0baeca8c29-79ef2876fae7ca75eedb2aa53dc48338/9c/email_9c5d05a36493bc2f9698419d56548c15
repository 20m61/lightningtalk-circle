  // Helper methods
  formatDate(dateString) {
    /* istanbul ignore next */
    cov_p6bhxuahv().f[15]++;
    const date =
    /* istanbul ignore next */
    (cov_p6bhxuahv().s[50]++, new Date(dateString));
    /* istanbul ignore next */
    cov_p6bhxuahv().s[51]++;
    return date.toLocaleDateString('ja-JP', {
      year: 'numeric',
      month: 'long',
      day: 'numeric',
      weekday: 'long',
      hour: '2-digit',
      minute: '2-digit'
    });
  }
