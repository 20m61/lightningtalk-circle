  }
  htmlToText(html) {
    /* istanbul ignore next */
    cov_236bacze16().f[13]++;
    cov_236bacze16().s[80]++;
    // Simple HTML to text conversion
    return html.replace(/<style[^>]*>.*?<\/style>/gi, '').replace(/<script[^>]*>.*?<\/script>/gi, '').replace(/<[^>]+>/g, '').replace(/\s+/g, ' ').trim();
  }
