      /* istanbul ignore next */
      cov_236bacze16().s[53]++;
      return {
        success: true,
        timestamp: new Date().toISOString()
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_236bacze16().s[54]++;
      console.error('Email sending failed:', error);
      /* istanbul ignore next */
      cov_236bacze16().s[55]++;
      throw error;
    }
  }
  async sendEmailDirect(emailData, templateData =
  /* istanbul ignore next */
  (cov_236bacze16().b[9][0]++, {})) {
    /* istanbul ignore next */
    cov_236bacze16().f[9]++;
    const html =
    /* istanbul ignore next */
    (cov_236bacze16().s[56]++, this.renderTemplate(emailData.template, templateData));
    const mailData =
    /* istanbul ignore next */
    (cov_236bacze16().s[57]++, {
      to: emailData.to,
      from:
      /* istanbul ignore next */
      (cov_236bacze16().b[10][0]++, emailData.from) ||
      /* istanbul ignore next */
      (cov_236bacze16().b[10][1]++, this.from),
      subject: emailData.subject,
      text: this.htmlToText(html),
      html: html,
      categories:
      /* istanbul ignore next */
      (cov_236bacze16().b[11][0]++, emailData.categories) ||
      /* istanbul ignore next */
      (cov_236bacze16().b[11][1]++, ['lightning-talk']),
      customArgs: {
        eventId: templateData.eventId,
        participantId: templateData.participantId,
        emailType: emailData.template
      }
    });
    /* istanbul ignore next */
    cov_236bacze16().s[58]++;
    switch (this.provider) {
      case 'sendgrid':
        /* istanbul ignore next */
        cov_236bacze16().b[12][0]++;
        cov_236bacze16().s[59]++;
        return await sgMail.send(mailData);
      case 'ses':
        /* istanbul ignore next */
        cov_236bacze16().b[12][1]++;
        cov_236bacze16().s[60]++;
        return await this.sendWithSES(mailData);
      default:
        /* istanbul ignore next */
        cov_236bacze16().b[12][2]++;
        cov_236bacze16().s[61]++;
        throw new Error(`Unknown email provider: ${this.provider}`);
    }
  }

  // Queue-based email sending (recommended for production)
  async sendEmail(emailType, to, templateData =
  /* istanbul ignore next */
  (cov_236bacze16().b[13][0]++, {}), options =
  /* istanbul ignore next */
  (cov_236bacze16().b[14][0]++, {})) {
    /* istanbul ignore next */
    cov_236bacze16().f[10]++;
    cov_236bacze16().s[62]++;
