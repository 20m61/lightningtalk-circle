  // Bulk email operations
  async sendBulkEmail(emailType, recipients, templateData =
  /* istanbul ignore next */
  (cov_236bacze16().b[23][0]++, {}), options =
  /* istanbul ignore next */
  (cov_236bacze16().b[24][0]++, {})) {
    /* istanbul ignore next */
    cov_236bacze16().f[15]++;
    const jobs =
    /* istanbul ignore next */
    (cov_236bacze16().s[82]++, []);
    /* istanbul ignore next */
    cov_236bacze16().s[83]++;
    for (const recipient of recipients) {
      const individualData =
      /* istanbul ignore next */
      (cov_236bacze16().s[84]++, {
        ...templateData,
        ...recipient
      });
      const job =
      /* istanbul ignore next */
      (cov_236bacze16().s[85]++, await this.sendEmail(emailType, recipient.email, individualData, {
        ...options,
        jobId: `bulk-${emailType}-${
        /* istanbul ignore next */
        (cov_236bacze16().b[25][0]++, recipient.id) ||
        /* istanbul ignore next */
        (cov_236bacze16().b[25][1]++, Date.now())}`
      }));
      /* istanbul ignore next */
      cov_236bacze16().s[86]++;
      jobs.push(job);
    }
    /* istanbul ignore next */
    cov_236bacze16().s[87]++;
    console.log(`📧 Bulk email queued: ${jobs.length} emails (${emailType})`);
    /* istanbul ignore next */
    cov_236bacze16().s[88]++;
    return {
      jobs,
      count: jobs.length
    };
  }

  // Queue management
  async getQueueStats() {
    /* istanbul ignore next */
    cov_236bacze16().f[16]++;
    cov_236bacze16().s[89]++;
    if (!this.queue) {
      /* istanbul ignore next */
      cov_236bacze16().b[26][0]++;
      cov_236bacze16().s[90]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_236bacze16().b[26][1]++;
    }
    const [waiting, active, completed, failed] =
    /* istanbul ignore next */
    (cov_236bacze16().s[91]++, await Promise.all([this.queue.getWaiting(), this.queue.getActive(), this.queue.getCompleted(), this.queue.getFailed()]));
    /* istanbul ignore next */
    cov_236bacze16().s[92]++;
    return {
      waiting: waiting.length,
      active: active.length,
      completed: completed.length,
      failed: failed.length,
      total: waiting.length + active.length + completed.length + failed.length
    };
  }
  async pauseQueue() {
    /* istanbul ignore next */
    cov_236bacze16().f[17]++;
    cov_236bacze16().s[93]++;
    await this.queue.pause();
    /* istanbul ignore next */
    cov_236bacze16().s[94]++;
    console.log('⏸️ Email queue paused');
  }
  async resumeQueue() {
    /* istanbul ignore next */
    cov_236bacze16().f[18]++;
    cov_236bacze16().s[95]++;
    await this.queue.resume();
    /* istanbul ignore next */
    cov_236bacze16().s[96]++;
    console.log('▶️ Email queue resumed');
  }
  async retryFailedJobs() {
    /* istanbul ignore next */
    cov_236bacze16().f[19]++;
    const failed =
    /* istanbul ignore next */
    (cov_236bacze16().s[97]++, await this.queue.getFailed());
    /* istanbul ignore next */
    cov_236bacze16().s[98]++;
    for (const job of failed) {
      /* istanbul ignore next */
      cov_236bacze16().s[99]++;
      await job.retry();
    }
    /* istanbul ignore next */
    cov_236bacze16().s[100]++;
    console.log(`🔄 Retrying ${failed.length} failed email jobs`);
  }

  // Cleanup
  async close() {
    /* istanbul ignore next */
    cov_236bacze16().f[20]++;
    cov_236bacze16().s[101]++;
    if (this.queue) {
      /* istanbul ignore next */
      cov_236bacze16().b[27][0]++;
      cov_236bacze16().s[102]++;
      await this.queue.close();
      /* istanbul ignore next */
      cov_236bacze16().s[103]++;
      console.log('📧 Email service closed');
    } else
    /* istanbul ignore next */
    {
      cov_236bacze16().b[27][1]++;
    }
  }

  // Health check
  async healthCheck() {
    /* istanbul ignore next */
    cov_236bacze16().f[21]++;
    cov_236bacze16().s[104]++;
    try {
      const stats =
      /* istanbul ignore next */
      (cov_236bacze16().s[105]++, await this.getQueueStats());
      /* istanbul ignore next */
      cov_236bacze16().s[106]++;
      return {
        status: 'healthy',
        provider: this.provider,
        enabled: this.enabled,
        queue: stats,
        timestamp: new Date().toISOString()
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_236bacze16().s[107]++;
      return {
        status: 'unhealthy',
        error: error.message,
        timestamp: new Date().toISOString()
      };
    }
  }
}
export default ProductionEmailService;
