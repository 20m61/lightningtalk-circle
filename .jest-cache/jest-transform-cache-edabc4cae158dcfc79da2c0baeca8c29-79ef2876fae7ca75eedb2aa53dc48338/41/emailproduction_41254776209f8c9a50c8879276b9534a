    {
      cov_236bacze16().b[16][1]++;
    }
    const jobData =
    /* istanbul ignore next */
    (cov_236bacze16().s[68]++, {
      emailData: {
        to,
        subject: template.subject,
        template: emailType,
        categories: [`lightning-talk-${emailType}`]
      },
      templateData
    });

    // Add to queue with priority and delay options
    const job =
    /* istanbul ignore next */
    (cov_236bacze16().s[69]++, await this.queue.add('send', jobData, {
      priority:
      /* istanbul ignore next */
      (cov_236bacze16().b[17][0]++, options.priority) ||
      /* istanbul ignore next */
      (cov_236bacze16().b[17][1]++, 0),
      delay:
      /* istanbul ignore next */
      (cov_236bacze16().b[18][0]++, options.delay) ||
      /* istanbul ignore next */
      (cov_236bacze16().b[18][1]++, 0),
      attempts:
      /* istanbul ignore next */
      (cov_236bacze16().b[19][0]++, options.attempts) ||
      /* istanbul ignore next */
      (cov_236bacze16().b[19][1]++, 3),
      jobId:
      /* istanbul ignore next */
      (cov_236bacze16().b[20][0]++, options.jobId) ||
      /* istanbul ignore next */
      (cov_236bacze16().b[20][1]++, `${emailType}-${Date.now()}-${Math.random().toString(36).substr(2, 9)}`)
    }));
    /* istanbul ignore next */
    cov_236bacze16().s[70]++;
    console.log(`📧 Email job queued: ${job.id} (${emailType} to ${to})`);
    /* istanbul ignore next */
    cov_236bacze16().s[71]++;
    return {
      jobId: job.id,
      queued: true
    };
  }

  // Template rendering
  renderTemplate(templateType, data) {
    /* istanbul ignore next */
    cov_236bacze16().f[11]++;
    const template =
    /* istanbul ignore next */
