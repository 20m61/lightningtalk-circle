  async setupQueue() {
    /* istanbul ignore next */
    cov_236bacze16().f[4]++;
    cov_236bacze16().s[32]++;
    try {
      // Create Redis connection for queue
      const redisClient =
      /* istanbul ignore next */
      (cov_236bacze16().s[33]++, Redis.createClient({
        url: this.redisUrl
      }));
      /* istanbul ignore next */
      cov_236bacze16().s[34]++;
      await redisClient.connect();

      // Create email queue
      /* istanbul ignore next */
      cov_236bacze16().s[35]++;
      this.queue = new Queue('email processing', this.redisUrl, {
        defaultJobOptions: {
          attempts: 3,
          backoff: {
            type: 'exponential',
            delay: 2000
          },
          removeOnComplete: 100,
          removeOnFail: 50
        },
        settings: {
          stalledInterval: 30000,
          maxStalledCount: 1
        }
      });

      // Process email jobs
      /* istanbul ignore next */
      cov_236bacze16().s[36]++;
      this.queue.process('send', this.processEmailJob.bind(this));

      // Queue event handlers
      /* istanbul ignore next */
      cov_236bacze16().s[37]++;
      this.queue.on('completed', job => {
        /* istanbul ignore next */
        cov_236bacze16().f[5]++;
        cov_236bacze16().s[38]++;
        console.log(`📧 Email job ${job.id} completed`);
        /* istanbul ignore next */
        cov_236bacze16().s[39]++;
        this.emit('emailSent', {
          jobId: job.id,
          email: job.data.to
        });
      });
      /* istanbul ignore next */
      cov_236bacze16().s[40]++;
      this.queue.on('failed', (job, err) => {
        /* istanbul ignore next */
        cov_236bacze16().f[6]++;
        cov_236bacze16().s[41]++;
        console.error(`❌ Email job ${job.id} failed:`, err.message);
        /* istanbul ignore next */
        cov_236bacze16().s[42]++;
        this.emit('emailFailed', {
          jobId: job.id,
          email: job.data.to,
          error: err.message
        });
      });
      /* istanbul ignore next */
      cov_236bacze16().s[43]++;
      this.queue.on('stalled', job => {
        /* istanbul ignore next */
        cov_236bacze16().f[7]++;
        cov_236bacze16().s[44]++;
        console.warn(`⚠️ Email job ${job.id} stalled`);
      });
      /* istanbul ignore next */
      cov_236bacze16().s[45]++;
      console.log('🔄 Email queue system initialized');
      /* istanbul ignore next */
      cov_236bacze16().s[46]++;
      this.initialized = true;
      /* istanbul ignore next */
      cov_236bacze16().s[47]++;
      this.emit('ready');
    } catch (error) {
      /* istanbul ignore next */
      cov_236bacze16().s[48]++;
      console.error('❌ Email queue setup failed:', error);
      /* istanbul ignore next */
      cov_236bacze16().s[49]++;
      throw error;
    }
  }
  async processEmailJob(job) {
    /* istanbul ignore next */
    cov_236bacze16().f[8]++;
    const {
      emailData,
      templateData
    } =
    /* istanbul ignore next */
