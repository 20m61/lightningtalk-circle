require('dotenv').config();
async function configureWordPress() {
  /* istanbul ignore next */
  cov_mjcrf4mst().f[0]++;
  const fetch =
  /* istanbul ignore next */
  (cov_mjcrf4mst().s[1]++, (await import('node-fetch')).default);
  const authString =
  /* istanbul ignore next */
  (cov_mjcrf4mst().s[2]++, Buffer.from(`${process.env.WP_USERNAME}:${process.env.WP_APP_PASSWORD}`).toString('base64'));
  /* istanbul ignore next */
  cov_mjcrf4mst().s[3]++;
  console.log('⚙️ WordPress設定自動化');
  /* istanbul ignore next */
  cov_mjcrf4mst().s[4]++;
  console.log('=====================');
  /* istanbul ignore next */
  cov_mjcrf4mst().s[5]++;
  console.log('');
  /* istanbul ignore next */
  cov_mjcrf4mst().s[6]++;
  try {
    /* istanbul ignore next */
    cov_mjcrf4mst().s[7]++;
    // 1. パーマリンク設定
    console.log('1. パーマリンク設定...');
    const permalinkResponse =
    /* istanbul ignore next */
    (cov_mjcrf4mst().s[8]++, await fetch(`${process.env.WP_API_URL}/settings`, {
      method: 'POST',
      headers: {
        'Authorization': `Basic ${authString}`,
        'Content-Type': 'application/json'
      },
      body: JSON.stringify({
        permalink_structure: '/%postname%/'
      })
    }));
    /* istanbul ignore next */
    cov_mjcrf4mst().s[9]++;
    if (permalinkResponse.ok) {
      /* istanbul ignore next */
      cov_mjcrf4mst().b[0][0]++;
      cov_mjcrf4mst().s[10]++;
      console.log('✅ パーマリンクを投稿名形式に設定');
    } else {
      /* istanbul ignore next */
      cov_mjcrf4mst().b[0][1]++;
      cov_mjcrf4mst().s[11]++;
      console.log('⚠️ パーマリンク設定: 手動で設定してください');
    }

    // 2. サイトタイトル確認・設定
    /* istanbul ignore next */
    cov_mjcrf4mst().s[12]++;
    console.log('\\n2. サイト設定確認...');
    const siteResponse =
    /* istanbul ignore next */
    (cov_mjcrf4mst().s[13]++, await fetch(`${process.env.WP_API_URL}/settings`, {
      headers: {
        'Authorization': `Basic ${authString}`
      }
    }));
    /* istanbul ignore next */
    cov_mjcrf4mst().s[14]++;
    if (siteResponse.ok) {
      /* istanbul ignore next */
      cov_mjcrf4mst().b[1][0]++;
      const siteData =
      /* istanbul ignore next */
      (cov_mjcrf4mst().s[15]++, await siteResponse.json());
      /* istanbul ignore next */
      cov_mjcrf4mst().s[16]++;
      console.log(`✅ サイト名: ${siteData.title}`);
      /* istanbul ignore next */
      cov_mjcrf4mst().s[17]++;
      console.log(`✅ URL: ${siteData.url}`);
    } else
    /* istanbul ignore next */
    {
      cov_mjcrf4mst().b[1][1]++;
    }
    cov_mjcrf4mst().s[18]++;
    console.log('\\n✅ WordPress基本設定完了');
    /* istanbul ignore next */
    cov_mjcrf4mst().s[19]++;
